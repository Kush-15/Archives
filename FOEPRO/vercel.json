{
  "version": 2,
  "buildCommand": "python manage.py collectstatic --noinput",
  "functions": {
    "FOEPRO/wsgi.py": {
      "runtime": "python3.12",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "FOEPRO/wsgi.py"
    }
  ]
}
